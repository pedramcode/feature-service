generator client {
  provider = "prisma-client-js"
  output   = "./src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Feature {
  id       String  @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  key      String  @unique
  isActive Boolean @default(false)
  desc     String?
}
